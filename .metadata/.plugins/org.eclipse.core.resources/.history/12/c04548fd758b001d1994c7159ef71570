package JavaPratice;

public class UniqueArray {

	public static void main(String[] args) {
		int a[]= {2,3,4,2,2,2,2,2};
		int s=0;
		int t=0;
		 for(int i=0;i<a.length;i++)
		 {
			 for(int j=i+1;j<a.length;j++) {
				 if(a[i]==a[j]) {
					 s=a[i]; 
				 }else {
					 t=a[i];
				 }
			 }
		 }System.out.println(" unique no is " +t);
	}

}
