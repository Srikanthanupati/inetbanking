package JavaPratice;

public class UniqueArray {

	public static void main(String[] args) {
		int a[]= {2,3,4,2,2,2,2,2};
		int k=0;
		int s=0;
		 for(int i=0;i<a.length;i++)
		 {
			 for(int j=i+1;j<a.length;j++) {
				 if(a[i]==a[j]) {
					 k++;
					 s=a[i];
					 
				 }
			 }
		 }System.out.println(k + " unique no is " +s);
	}

}
