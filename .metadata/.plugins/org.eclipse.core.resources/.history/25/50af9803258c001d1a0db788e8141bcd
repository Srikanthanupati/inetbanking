package JavaPratice;

import java.util.ArrayList;
import java.util.HashSet;

public class Practice {

	public static void main(String[] args) {
		String a="Welcomel";
		String dups="";
		HashSet ab=new HashSet();
		
		for(int i=0;i<8;i++)
		{
				if(a.charAt(i)==a.charAt(i+1))
						{
					dups=dups+a.charAt(i);
					
						}
			}System.out.println(dups);
		}
}
