package JavaPratice;
import java.util.*;

public class Sumofset {

	public static void main(String[] args) 
	{
		
	
		
		Set<Integer> i  = new HashSet<Integer>();
		i.add(25);
		i.add(50);
		i.add(25);
		i.add(58);
		i.add(2);
		i.add(5);
		
		
		System.out.println(i);
		
		Object[] a=i.toArray();
		
		Integer[] integerArray = new Integer[a.length];
		
		int x;
		for (int x = 0; x < a.length; x++)
		{
            integerArray[x] = (Integer)a[x];
        }
		
		int j;
		int b=0;
		
		for(j=0;j<=integerArray.length-1;j++) 
		{
			
			b = b + integerArray[j];
		}
		System.out.print(b);
		
		 
		
		
	}

}
